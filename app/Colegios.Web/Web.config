    <add key="TopPostsRead" value="4"/>
    <add key="ActivitiesPerPage" value="3"/>
    <add key="homePageNumPosts" value="4"/>
    <add key="homePageNumActivities" value="3"/>
    <add key="showOpenDays" value="false"/>
    <add key="highLightedNurseries" value="3"/>
