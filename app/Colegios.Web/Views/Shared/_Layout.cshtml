                  <li class="lnkUnSelected"><a id="lnkBlogs" href="@Url.Action("Index", "Home", new { area = "blog" })">Blog</a><div class="lnkArrow" style="display:none"></li>
              </ul>
              <a href="/colegios/notas" style="font-size:20px;font-weight:bold;width:200px;margin-left:20px; color:red;font-family: 'Times New Roman'">Notas Cdi 2012</a>
            </nav>
            <!-- social networks -->
            <div class="homeSocialLinks">
                <a href="http://www.facebook.com/colesyguardes" target="_blank"><img id="facebookIcon" alt="Coles y Guardes Página Facebook" title="Facebook" width="40" height="40" src="@Url.Content("/Content/themes/base/social/facebook.png")" style="margin-top:10px;margin-left:40px" /></a>
                <img alt="GooglePlus" title="GooglePlus" width="40" height="40" src="@Url.Content("/Content/themes/base/social/googleplus.png")" style="margin-top:10px;" />
                <!--<img alt="Twitter" title="Twitter" width="40" height="40" src="@Url.Content("/Content/themes/base/social/tuenti.png")" style="margin-top:10px"/>-->


                <div id="facebookLike" style="float:right;margin-top: -18px;margin-left:10px;z-index:20;">
                    <ul style="list-style:none;margin-left:0;padding-left:0">
                        <li style="margin-left:5;padding-left:5">
@*                        <fb:like href="http://www.colesyguardes.es" send="false" width="100px" show_faces="false" font="tahoma" layout="button_count"></fb:like>*@
<div class="fb-like" data-href="http://www.facebook.com/colesyguardes" data-send="false" data-layout="button_count" data-width="100" data-show-faces="false" data-font="tahoma"></div>
                        </li>
                        <!--<li style="margin-left:5;padding-left:5"><a href="http://www.tuenti.com/share" id="tuenti_share" style="padding-top: 20px"></a></li>-->
                        <li style="margin-left:5;padding-left:5">
                            <g:plusone href="http://www.colesyguardes.es"></g:plusone>
                        </li>
                    </ul>
                </div>
            </div>
            <!-- social networks -->
            @RenderBody()
        </section>
        <footer>
            <center><a id="lnkHome" href="@Url.Action("Index", "School", new { area = "" })"><span style="font-family:Carmen;font-size:20px">ColesyGuardes.es</span></a></center><br>
