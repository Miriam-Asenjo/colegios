    <!-- google plus -->
    <script type="text/javascript" src="https://apis.google.com/js/plusone.js"></script>
    <!-- google analytics -->
    <script type="text/javascript">
        var _gaq = _gaq || [];
        _gaq.push(['_setAccount', 'UA-28776023-1']);
        _gaq.push(['_trackPageview']);


        window.fbAsyncInit = function () {
            FB.init({
                xfbml: true
            })
        };
        // Load the SDK Asynchronously
        (function (d) {
            //if (!($.browser.msie && parseInt($.browser.version, 10) === 8)) {
            if (!($.browser.msie)) {
                //console.log("Loading facebook");
                var js, id = 'facebook-jssdk', ref = d.getElementsByTagName('script')[0];
                if (d.getElementById(id)) { return; }
                js = d.createElement('script'); js.id = id; js.async = true;
                js.src = "//connect.facebook.net/es_ES/all.js";
                ref.parentNode.insertBefore(js, ref);
            } 
        } (document));
    </script>
</head>
<body> <div id="fb-root"></div>
    <div class="page">
        <a id="lnkHome" href="@Url.Action("Index", "School", new { area = "" })"><header class="topHeader clearfix">
</header></a>
        <section id="main">
            <nav style="width:75%">
              <ul>
                  <li class="lnkUnSelected"><a id="lnkHome" href="@Url.Action("Index", "School", new { area = "" })"><img src="@Url.Content("/Content/themes/base/images/home/icono-home.png")" width="48" height="47" style="margin-bottom:0px;border:0">&nbsp;</a>
                  <div class="lnkArrow" style="display:none;margin-top:-9px"></div></li>
                  <li class="lnkUnSelected"><a id="lnkColegios" href="@Url.RouteUrl("SchoolSearch")">Colegios</a><div class="lnkArrow" style="display:none"></li>    
